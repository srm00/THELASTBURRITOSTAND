
public class Burrito {
	
	
	public double calcPrice()
	{
		double price = 0; 
		MaxHeap1 priceHeap = new MaxHeap1(8);
		priceHeap.insert(this.getPrice(cookedBurrito[0])); //cookedBurrito is the completed array of Strings of...
		priceHeap.insert(this.getPrice(cookedBurrito[1])); //...what the customer wanted (w/out blanks)
		priceHeap.insert(this.getPrice(cookedBurrito[2]));
		priceHeap.insert(this.getPrice(cookedBurrito[3]));
		priceHeap.insert(this.getPrice(cookedBurrito[4]));
		priceHeap.insert(this.getPrice(cookedBurrito[5]));
		priceHeap.insert(this.getPrice(cookedBurrito[6]));
		priceHeap.insert(this.getPrice(cookedBurrito[7]));
		price = priceHeap.deleteMax(); 
		price = price + priceHeap.deleteMax(); 
	    price = price + priceHeap.deleteMax();
		
		
		return price; 
	}
	

	
	
	 //Sample call of calcPrice
	//burritoOne.calcPrice() 
	//returns the price of the associated with burritoOne
}
